<launch>
    <node pkg="lidar_camera_calib" type="pointcloud_filter.py" name="pointcloud_filter_node" output="screen">
        
        <param name="input_topic" value="/livox/lidar" />
        <param name="output_topic" value="/filtered_points" />

        <param name="x_min" value="0.5" />
        <param name="x_max" value="20.0" />

        <param name="z_min" value="-0.5" />
        <param name="z_max" value="1.5" />

        <param name="angle_min" value="-30.0" />
        <param name="angle_max" value="30.0" />

    </node>
</launch>